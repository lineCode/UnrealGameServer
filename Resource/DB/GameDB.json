        {
          "name": "@totalexp",
          "type": "int"
        },
        {
          "name": "@objectid",
          "type": "int"
        }
      ],
      "Body": "SET NOCOUNT ON; INSERT INTO [dbo].[Player]([playername] , [AccountId] , [Attack] , [Hp] , [Level] , [MaxHp] , [Speed] , [TotalExp] , [objectid]) VALUES((@name) , (@accountid) , (@attack) , (@hp) , (@level) , (@maxhp) , (@speed) ,(@totalexp) , (@objectid))"
    },
